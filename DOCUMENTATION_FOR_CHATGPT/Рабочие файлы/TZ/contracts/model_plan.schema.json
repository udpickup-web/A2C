{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "model_plan.json",
  "type": "object",
  "required": [
    "order",
    "units"
  ],
  "properties": {
    "order": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "minItems": 1
    },
    "units": {
      "type": "string",
      "enum": [
        "mm",
        "in"
      ]
    },
    "notes": {
      "type": "string"
    }
  },
  "additionalProperties": false
}
